var el = $('#favorite-button-notice');
el.empty();
<% if @favorite.errors.empty? %>
    el.append('<p class="text-success small">お気に入り登録しました</p>');
    $('#favorite-button').html("<%= escape_javascript(render('favorites/favorite_button', user: @favorite.to_user)) %>")
<% else %>
    el.append('<p class="text-success small">お気に入り登録に失敗しました</p>');
<% end %>
